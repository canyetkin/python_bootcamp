   ],
   "source": [
    "def sum_double(a, b):\n",
    "  if a!=b:\n",
    "    return a+b\n",
    "  else:\n",
    "    return (a+b)*2\n",
    "  \n",
    "sum_double(10,10)"
   ]
  }
